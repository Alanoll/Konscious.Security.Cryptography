{
  "version": "1.0.4-*",
  "authors": [ "Keef Aragon" ],
  "title": "Konscious.Security.Cryptography.Blake2",
  "buildOptions": {
    "debugType": "portable",
    "emitEntryPoint": false,
    "warningsAsErrors": true,
    "xmlDoc": true,
    "additionalArguments": [
      "/unsafe",
      "/ruleset:../fxcop.ruleset"
    ]
  },
  "dependencies":
  {
      "Microsoft.CodeAnalysis.FxCopAnalyzers": {
        "version": "1.1.*",
        "type": "build"
      }
  },
  "frameworks": {
    "netcoreapp1.0": {
      "dependencies": {
        "Microsoft.NETCore.App": {
          "type": "platform",
          "version": "1.0.0"
        }
      },
      "imports": "dnxcore50"
    },
    "net451": {
      "dependencies": {
          "System.Numerics.Vectors": "4.*"
      }
    },
    "netstandard1.3": {
      "dependencies": {
        "NETStandard.Library": "1.3.0",
        "System.Security.Cryptography.Algorithms": "4.2.0",
        "System.Numerics.Vectors": "4.*"
      },
      "imports": "net46"
    }
  },
  "description": "An implementation of Blake2 for .NET that integrates with the standard crypto libraries",
  "projectUrl": "https://github.com/kmaragon/Konscious.Security.Cryptography",
  "copyright": "(c) 2017 Keef Aragon",
  "packOptions": {
    "owners": [
      "Keef Aragon"
    ],
    "summary": "Blake2b library for .NET",
    "licenseUrl": "https://github.com/kmaragon/Konscious.Security.Cryptography/blob/master/LICENSE",
    "repository": {
      "type": "git",
      "url": "https://github.com/kmaragon/Konscious.Security.Cryptography.git"
    },
    "tags": [
      "crypto",
      "cryptography",
      "hash",
      "blake2",
      "blake2b"
    ]
  }
}
