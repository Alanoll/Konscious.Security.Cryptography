{
  "version": "1.0.4-*",
  "testRunner": "xunit",
  "buildOptions": {
    "debugType": "portable",
    "emitEntryPoint": false,
    "warningsAsErrors": true,
    "xmlDoc": false,
    "additionalArguments": [
      "/ruleset:../fxcop.ruleset"
    ]
  },
  "dependencies":
  {
      "Konscious.Security.Cryptography.Blake2": "1.0.*",
      "xunit": "2.1.0",
      "Microsoft.CodeAnalysis.FxCopAnalyzers": {
        "version": "1.1.*",
        "type": "build"
      }
  },
  "frameworks": {
    "netcoreapp1.1": {
      "dependencies": {
        "Microsoft.NETCore.App": {
          "type": "platform",
          "version": "1.1.0"
        },
        "dotnet-test-xunit": {
          "version": "1.0.0-*",
          "type": "build"
        }
      },
      "imports": [
        "dnxcore50",
        "portable-net45+win8"
      ]
    }
  }
}
